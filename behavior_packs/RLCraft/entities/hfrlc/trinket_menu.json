{
  "format_version": "1.10",
  "minecraft:entity": {
    "description": {
      "identifier": "trinket_menu",
      "is_spawnable": false,
      "is_summonable": false,
      "is_experimental": false
    },
    "components": {
      "minecraft:type_family": {
        "family": ["trinket_menu", "ui_entity"]
      },
      "minecraft:variant": {
        "value": 0
      },
      "minecraft:mark_variant": {
        "value": 0
      }
    },
    "component_groups": {
      "active_trinket": {
        "minecraft:variant": {
          "value": 1
        }
      },
      "duplicate_trinket": {
        "minecraft:variant": {
          "value": 2
        }
      }
    },
    "events": {
      "mark_as_active": {
        "add": {
          "component_groups": ["active_trinket"]
        }
      },
      "mark_as_duplicate": {
        "add": {
          "component_groups": ["duplicate_trinket"]
        }
      },
      "cleanup_duplicates": {
        "filters": {
          "test": "has_component",
          "subject": "self",
          "value": "duplicate_trinket"
        },
        "add": {
          "component_groups": ["minecraft:despawn"]
        }
      }
    }
  }
}